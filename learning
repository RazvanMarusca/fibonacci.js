function fibonacciOutput(n) {
  let a = 0;
  let b = 1;
  let temp;

  if (n === 0) {
    console.log("Fibonacci(0) = 0");
    return a;
  }

  console.log("Fibonacci(0) = 0");
  console.log("Fibonacci(1) = 1");

  for (let i = 2; i <= n; i++) {
    temp = a + b;
    a = b;
    b = temp;
    console.log("Fibonacci(" + i + ") = " + b);
  }
}

fibonacciOutput(10);
